{
    "name": "natepage/standards",
    "description": "Package to run coding standards tests.",
    "type": "package",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "symplify/package-builder": "^5.0",
        "eoneopay/utils": "dev-master",
        "symfony/process": "^4.1",
        "phpunit/phpunit": "^7.3",
        "phpstan/phpstan": "^0.10.3",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.1",
        "brianium/paratest": "^2.0",
        "symplify/easy-coding-standard": "^5.0",
        "natepage/sniffs": "^1.0"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "suggest": {
        "ext-xdebug": "Calculate code coverage of unit tests",
        "squizlabs/php_codesniffer": "Check code style"
    },
    "bin": [
        "bin/standards"
    ],
    "autoload": {
        "psr-4": { "NatePage\\Standards\\": "src/" }
    }
}
